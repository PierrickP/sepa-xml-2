    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PierrickP/sepa-xml-2/issues"
  },
  "homepage": "https://github.com/PierrickP/sepa-xml-2",
  "dependencies": {
    "handlebars": "^4.0.5"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.8",
    "istanbul": "^0.4.2",
    "jaguarjs-jsdoc": "0.0.1",
    "jsdoc": "^3.4.0",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "^1.2.0",
    "xml2js": "^0.4.16"
