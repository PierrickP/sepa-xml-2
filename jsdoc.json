{
  "opts": {
    "template": "node_modules/jaguarjs-jsdoc",
    "destination": "./doc/",
    "recurse": true,
    "readme": "README.md"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["lib"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "applicationName": "Sepa-XML-2",
    "linenums": true
  }
}
